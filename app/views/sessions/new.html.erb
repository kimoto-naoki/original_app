<h1>ログイン</h1>
<%= form_with(url: login_path, scope: :session, local: true) do |f| %>
<% if flash[:danger].present? %>
  <%= flash[:danger] %>
<% end %>
  
  <%= f.label :player_name %>
  <%= f.text_field :player_name %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.submit "ログイン" %>
<% end %>